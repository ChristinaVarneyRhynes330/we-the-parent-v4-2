{
  "tools": {
    "core": [
      "FileReadTool",
      "FileWriteTool"
    ],
    "exclude": [
      "ShellTool(rm -rf)",
      "ShellTool(sudo)",
      "ShellTool(del)",
      "ShellTool(format)",
      "ShellTool(shutdown)",
      "ShellTool(reboot)"
    ]
  },
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-supabase"
      ],
      "env": {
        "SUPABASE_URL": "https://your-supabase-url.supabase.co",
        "SUPABASE_ANON_KEY": "your-anon-key"
      }
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your-github-token"
      }
    }
  },
  "agentYoloMode": false
}